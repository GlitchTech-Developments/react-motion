{
	"name": "@glitchtech-dev/react-motion",
	"version": "1.4.3",
	"description": "A simple wrapper of Motion for React.",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/*.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"dist",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"keywords": [
		"motion",
		"react",
		"wrapper"
	],
	"author": "GlitchTech Developments",
	"license": "MIT",
	"scripts": {
		"build": "tsup",
		"lint": "eslint --ignore-path .gitignore . --ext ts",
		"package:publish": "pnpm run lint && tsup && standard-version && npm publish --access public"
	},
	"peerDependencies": {
		"motion": "^10.16.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"typescript": "^5.1.3"
	},
	"devDependencies": {
		"@types/node": "^20.2.5",
		"@types/react": "^18.2.8",
		"@types/react-dom": "^18.2.4",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"eslint": "^8.42.0",
		"eslint-plugin-react": "^7.32.2",
		"standard-version": "^9.5.0",
		"tsup": "^6.7.0"
	},
	"homepage": "https://github.com/GlitchTech-Developments/react-motion#readme",
	"bugs": {
		"url": "https://github.com/GlitchTech-Developments/react-motion/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/GlitchTech-Developments/react-motion.git"
	}
}
